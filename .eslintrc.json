{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-use-before-define": 0,
        // "indent": "off",
        // "react / jsx-indent": [2,2],
        "react/prop-types": ["error", { "ignore": ["navigation"] }],
        "import/prefer-default-export": 0
    }
}
